							{{{this.body}}}
				{{/with}}
			{{/each}}
							<hr>
							<div class="ads">
			{{#each info}}
				{{#with two}}
								<h2>{{this.title}}</h2>
								{{{this.body}}}
				{{/with}}
			{{/each}}
							</div>
			{{#each info}}
				{{#with three}}				
							<h2>{{this.title}}</h2>
							{{{this.body}}}
				{{/with}}
			{{/each}}
						</div>
					</div>
				</div>
			</div>
		</div>
		{{> footer}}
	</div>
</body>
</html>